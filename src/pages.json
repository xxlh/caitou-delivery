{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/deal",
			"style": {
				"navigationBarTitleText": "配送订单",
                "enablePullDownRefresh": true,
				"backgroundColor": "#fafafa"
			}
		}, {
			"path": "pages/index/deal-history",
			"style": {
				"navigationBarTitleText": "历史配送订单",
                "enablePullDownRefresh": true,
				"backgroundColor": "#eee"
			}
		}, {
			"path": "pages/index/deal-detail",
			"style": {
				"navigationBarTitleText": "配送单取货",
				"backgroundColor": "#eee"
			}
		}, {
			"path": "pages/direction/direction",
			"style": {
				"navigationBarTitleText": "配送线路规划",
                "enablePullDownRefresh": true,
				"backgroundColor": "#eee"
			}
		}, {
			"path": "pages/direction/packing-detail",
			"style": {
				"navigationBarTitleText": "取货点货单",
				"backgroundColor": "#eee"
			}
		}, {
			"path": "pages/location-poc/web",
			"style": {
				"navigationBarTitleText": "uni location"
			}
		}, {
			"path": "pages/location-poc/native",
			"style": {
				"navigationBarTitleText": "native locaiton"
			}
		}, {
			"path": "pages/my/my",
			"style": {
				"navigationBarTitleText": "个人中心"
			}
		}, {
			"path": "pages/my/login",
			"style": {
				"navigationBarTitleText": "登陆"
			}
		}, {
			"path": "uni_modules/uni-upgrade-center-app/pages/upgrade-popup",
			"style": {
				"disableScroll": true,
				"app-plus": {
					"backgroundColorTop": "transparent",
					"background": "transparent",
					"titleNView": false,
					"scrollIndicator": false,
					"popGesture": "none",
					"animationType": "fade-in",
					"animationDuration": 200
	
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"easycom": {
        "autoscan": true,
        "custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue",
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		}
	},
	"tabBar": {
		"color": "#333333",
		"selectedColor": "#f06c7a",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
				"pagePath": "pages/index/deal",
				"iconPath": "static/tabs/home.png",
				"selectedIconPath": "static/tabs/home-on.png",
				"text": "订单"
			}, {
				"pagePath": "pages/direction/direction",
				"iconPath": "static/tabs/direction.png",
				"selectedIconPath": "static/tabs/direction-on.png",
				"text": "路线"
			}, {
				"pagePath": "pages/my/my",
				"iconPath": "static/tabs/user.png",
				"selectedIconPath": "static/tabs/user-on.png",
				"text": "我的"
			}
		]
	}
